{
  "manifest_version": 2,
  "name": "IOV - Chrome extension",
  "version": "0.0.0",
  "description": "IOV Chrome extension",
  "browser_action": {
    "default_title": "IOV - Extension",
    "default_popup": "index.html"
  },
  "applications": {
    "gecko": {
      "id": "admin@iov.one"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/contentScript.js"]
    }
  ],
  "externally_connectable": {
    "matches": ["http://localhost/*", "https://*.iov.one/*"]
  },
  "key": "",
  "background": {
    "scripts": ["js/backgroundScript.js"],
    "persistent": true
  },
  "permissions": ["<all_urls>"],
  "web_accessible_resources": ["js/*"],
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'sha256-qAcmzXQ8jmB7ddF3B7OWAJKIn2C00zGiCGj8W5dG+wo='; object-src 'self'"
}

{
  "name": "sanes-chrome-extension",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "format": "prettier --write --loglevel warn ./src/**/*.{ts,tsx,json,md,css}",
    "lint": "eslint -c .eslintrc.js 'src/**/*.ts{,x}'",
    "start": "react-scripts start",
    "test": "CI=true react-scripts test --env=jsdom",
    "watch": "cra-build-watch --verbose"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@types/jest": "24.0.6",
    "@types/node": "11.9.4",
    "@types/react": "16.8.3",
    "@types/react-dom": "16.8.2",
    "medulas-react-components": "*",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-scripts": "2.1.5"
  },
  "devDependencies": {
    "cra-build-watch": "ssh://git@github.com:apanizo/cra-build-watch#f8015a7e36b9f9307fe680dd57115417931d6ac6"
  }
}

{
  "manifest_version": 2,
  "name": "6uild Seal (development)",
  "description": "Key manager and transaction signer for the 6uild grafain toolset,",
  "browser_action": {
    "default_title": "6uild Seal  (development)",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  },
  "content_scripts": [],
  "externally_connectable": {
    "matches": ["http://localhost/*", "https://*.grafain.net/*", "https://*.6uild.net/*"]
  },
  "background": {
    "scripts": ["js/backgroundScript.js"],
    "persistent": true
  },
  "permissions": [],
  "web_accessible_resources": ["js/*"],
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'sha256-qAcmzXQ8jmB7ddF3B7OWAJKIn2C00zGiCGj8W5dG+wo='; object-src 'self'"
}
